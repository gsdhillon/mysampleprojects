<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <!--           MyDataServlet                     -->
    <servlet>
        <servlet-name>MyDataServlet</servlet-name>
        <servlet-class>javacode.services.MyDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyDataServlet</servlet-name>
        <url-pattern>/MyDataServlet</url-pattern>
    </servlet-mapping>
    <!--           LoginServlet                     -->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>javacode.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <!--           MusterServlet                     -->
    <servlet>
        <servlet-name>MusterServlet</servlet-name>
        <servlet-class>javacode.services.MusterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MusterServlet</servlet-name>
        <url-pattern>/MusterServlet</url-pattern>
    </servlet-mapping>
    <!--           UserMasterServlet                     -->
    <servlet>
        <servlet-name>UserMasterServlet</servlet-name>
        <servlet-class>javacode.reports.UserMasterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserMasterServlet</servlet-name>
        <url-pattern>/UserMasterServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
